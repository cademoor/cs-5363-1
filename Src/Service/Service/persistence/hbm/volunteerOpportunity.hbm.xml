<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="Ttu.Domain" assembly="Ttu.Domain">
    <class name="VolunteerOpportunity" table="VolunteerOpportunity" lazy="true">
        <id name="RecordId">
            <generator class="identity" />
        </id>

        <many-to-one name="CreatedBy" column="CreatedByUserRecordId" class="User" />

        <property name="StartTime" />
        <property name="StopTime" />

    </class>
</hibernate-mapping>